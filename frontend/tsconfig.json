{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": false,
    "module": "es2020",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./src/**/**/*"
  ]
}
